<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digimasters.rev_comsgh">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/revcol_header"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.digimasters.rev_comsgh.Login"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.digimasters.rev_comsgh.MainActivity"
            android:label="@string/new_customer"
            android:exported="true"/>
        <activity
            android:name="com.digimasters.rev_comsgh.summary"
            android:label="Summary" />
        <activity android:name="com.digimasters.rev_comsgh.Home" />
        <activity android:name="com.digimasters.rev_comsgh.report"></activity>
    </application>

</manifest>